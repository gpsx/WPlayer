<body>
<div class="main-menu">
	<header class="header">
		<a href="index.html" class="logo"><i class="ico ti-harddrive"></i>WPERFORANCE</a>
		<button type="button" class="button-close fa fa-times js__menu_close"></button>
	</header>
	<!-- /.header -->
	<div class="content">

		<div class="navigation">
			<ul class="menu js__accordion">
				<li>
					<a class="waves-effect" href="index.html"><i class="menu-icon ti-dashboard"></i><span>Home</span></a>
				</li>
				<li>
					<a class="waves-effect" href="index.html"><i class="menu-icon ti-dashboard"></i><span>Histórico</span></a>
				</li>
				<li>
					<a class="waves-effect" href="index.html"><i class="menu-icon ti-dashboard"></i><span>Perfil</span></a>
				</li>
				<li>
					<a class="waves-effect" href="index.html"><i class="menu-icon ti-dashboard"></i><span>Suporte</span></a>
				</li>
			</ul>
		</div>
		<!-- /.navigation -->
	</div>
	<!-- /.content -->
</div>
<!-- /.main-menu -->

<div class="fixed-navbar">
	<div class="pull-left">
		<button type="button" class="menu-mobile-button glyphicon glyphicon-menu-hamburger js__menu_mobile"></button>
		<h1 class="page-title">Todas as máquinas</h1>
		<!-- /.page-title -->
	</div>
	<!-- /.pull-left -->
</div>
<!-- /.fixed-navbar -->
<div id="wrapper">
	<div class="main-content">
		<div class="row small-spacing">
			<div class="col-xs-12">
				<div class="box-content">
					<h4 class="box-title">Histórico</h4>
					<table id="example" class="table table-striped table-bordered display" style="width:100%">
						<thead>
							<tr>
								<th>Nome da máquina</th>
								<th>Tipo de dado</th>
								<th>Porcentagem</th>
								<th>Horário</th>
							</tr>
						</thead>
						<tfoot>
							<tr>
								<th>Nome da máquina</th>
								<th>Tipo de dado</th>
								<th>Porcentagem</th>
								<th>Horário e data</th>
							</tr>
						</tfoot>
						<tbody>
                            <% for(alert of alerts){ %>
                                <tr>
                                    <td><%= alert.APP_NAME%></td>
                                    <td><%= alert.APP_SHORT_DESCRIPTION%></td>
                                    <td><%= alert.APP_TYPE%></td>
                                    <td><%= alert.APP_FORMATED_PRICE%></td>
                                </tr>
                            <% } %>
							</tbody>
					</table>
				</div>
				<!-- /.box-content -->
			</div>
		</div>
	</div>
	<!-- /.main-content -->

</div>
	<!-- Placed at the end of the document so the pages load faster -->
	<script src="../../assets/scripts/jquery.min.js"></script>
	<script src="../../assets/scripts/modernizr.min.js"></script>
